{
    "name": "go-snippets",
    "displayName": "Go Snippets",
    "description": "Some snippets for go,especially usefull for leetcode etc.",
    "version": "0.1.4",
    "publisher": "banxi",
    "icon": "images/logo.png",
    "homepage": "https://github.com/banxi1988/vscode-go-snippets",
    "repository": {
        "type": "git",
        "url": "https://github.com/banxi1988/vscode-go-snippets.git"
    },
    "engines": {
        "vscode": "^1.14.0"
    },
    "categories": [
        "Snippets"
    ],
    "contributes": {
        "snippets": [
            {
                "language": "go",
                "path": "./snippets/leetcode.json"
            },
            {
                "language": "go",
                "path": "./snippets/go.json"
            }
        ]
    },
    "scripts": {
        "build": "node ./snippets-src/make_snippets.js",
        "vscode:prepublish": "node ./snippets-src/make_snippets.js"
    }
}
